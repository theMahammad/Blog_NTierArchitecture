@using EntityLayer.Concrete;
@model List<Category>;

<table class="table table-bordered table-striped">
    <thead>

    
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
            <th>Status</th>
            <th>Delete</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.IsDeleted</td>
                <td><a href="#" class="btn btn-warning">Sil</a></td>
            </tr>
        }


    </tbody>
    </table>