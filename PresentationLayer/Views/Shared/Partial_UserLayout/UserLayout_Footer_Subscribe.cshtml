<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

<div class="col-lg-4 subscribe-main footer-grid-agileits-w3ls text-left">
				<h2>Signup to our newsletter</h2>
				<div class="subscribe-main text-left">
					<div class="subscribe-form">
						<form  method="post" class="subscribe_form">
							<input class="form-control" type="email" name="Email" id="EmailFooter" placeholder="Enter your email..." required="">
							<button id="submitButtonFooter" type="submit" class="btn btn-primary submit">Submit</button>
						</form>
						<div class="clearfix"> </div>
					</div>
					<p>We respect your privacy.No spam ever!</p>
				</div>
				<!-- //subscribe -->
			</div>

			<script>
				$('#submitButtonFooter').click(function (event) {
	
		event.preventDefault();
	var formData = new FormData()
	formData.append("Email", $('#EmailFooter').val())
	


	$.ajax({
		type: "POST",
		url: "/Subscriber/AddSubscriber",
		contentType: false,
		processData: false,
		data: formData,
		success: function (result) {

				swal(result.title,result.text)
				$('#EmailFooter').val('')

		},
		error: function (result) {
			console.log(result)
		}
	});
});
			</script>